{
  "name": "rjbl-fullstack",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "install:all": "npm --prefix backend install && npm --prefix frontend install",
    "dev": "concurrently \"npm --prefix backend start\" \"npm --prefix frontend dev\"",
    "dev:backend": "npm --prefix backend start",
    "dev:frontend": "npm --prefix frontend dev",
    "db:init": "psql -d rjbl -f ./schema.sql"
  },
  "devDependencies": {
    "concurrently": "^9.0.0"
  }
}
